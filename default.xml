    <project path="macaca/webdriver/server" name="webdriver-server.git"/>
    <project path="macaca/webdriver/java" name="wd.java.git"/>

    <!-- macacajs/python -->
    <project path="macaca/python/driver" name="wd.py.git"/>

    <!-- macacajs/docker -->
    <project path="macaca/docker/android-image" name="macaca-android-docker.git"/>

    <!-- macacajs/doc -->
