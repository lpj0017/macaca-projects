    <project path="macaca/gradle" name="gradle.git"/>
    <project path="macaca/utils" name="macaca-utils.git"/>
    <project path="macaca/driver-base" name="driver-base.git"/>
    <project path="macaca/uitest-nodejs" name="uitest.git"/>

    <!-- macacajs/datahub -->
    <project path="macaca/datahub/core" name="macaca-datahub.git"/>
    <project path="macaca/datahub/view" name="datahub-view.git"/>
