    <remote name="app-bootstrap" fetch="git@github.com:app-bootstrap/"/>

    <!-- default config of all projects -->
    <!-- remote: the base url -->
    <!-- revision:the branch name -->
    <default revision="refs/heads/master" remote="macacajs" sync-j="4"/>
